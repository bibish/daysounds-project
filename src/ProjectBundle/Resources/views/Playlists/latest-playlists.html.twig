<h2>Latest Playlists</h2>
<ul>
    {% block latestPlaylists %}
        {% for playlist in datas.latestPlaylists %}
            <li>
                <a  href="{{ path('playlist_sounds', {'id': playlist.id }) }}">{{ playlist.id }} {{ playlist.name }}</a>
                <ul class="action">
                    <li><a href="{{ path('delete_playlist', {'id': playlist.id }) }}">Delete</a></li>
                    <li><a href="{{ path('edit_playlist', {'id': playlist.id }) }}">edit</a></li>
                    <li><a href="{{ path('add_sound_to_playlist', {'id': playlist.id }) }}">add sound</a></li>
                </ul>
            </li>
        {% endfor %}
    {% endblock %}
</ul>
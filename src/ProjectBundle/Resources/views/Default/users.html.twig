{% extends "@ProjectBundle/Resources/views/Default/layout.html.twig" %}

{% block main %}
    <ul class="users-list">
        {% for user in datas.listeUsers %}
            <li>
                <ul>
                    <li>{{ user.username }}</li>
                    <li><a href="{{ path('add_follow',{'slug_username':user.slug}) }}">follow</a></li>
                </ul>
            </li>
        {% endfor %}
    </ul>
{% endblock %}